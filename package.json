{
    "name": "algorithm-test",
    "author": "jebitamami@gmail.com",
    "version": "1.0.0",
    "description": "flatten array & string to array",
    "main": "src/index.js",
    "scripts": {
        "start:watch": "nodemon --exec 'ts-node src/index.ts'",
        "start": "ts-node src/index.ts -1",
        "test": "jest"
    },
    "dependencies": {
        "@types/node": "10.12.18",
        "ts-node": "8.0.1",
        "typescript": "^4.9.5"
    },
    "devDependencies": {
        "@types/jest": "^29.4.0",
        "jest": "^29.4.3",
        "nodemon": "1.18.4",
        "ts-jest": "^29.0.5"
    },
    "keywords": [
        "flatten array",
        "node.js"
    ],
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "transform": {
            "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
        },
        "transformIgnorePatterns": [
            "node_modules/(?!variables/.*)"
        ]
    },
    "engines" : { 
        "npm" : ">=6.0.0 <9.0.0",
        "node" : ">=14.0.0 <17.0.0"
    }
}
